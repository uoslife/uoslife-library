{
	"name": "@uoslife/webview",
	"private": false,
	"version": "0.0.27",
	"type": "module",
	"packageManager": "pnpm@8.4.0",
	"main": "dist/@uoslife/webview.es.js",
	"module": "dist/@uoslife/webview.umd.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/@uoslife/webview.es.js",
			"require": "./dist/@uoslife/webview.umd.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist/*"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/uoslife/uoslife-library.git",
		"directory": "dist"
	},
	"publishConfig": {
		"exports": {
			".": {
				"import": "./dist/@uoslife/webview.es.js",
				"require": "./dist/@uoslife/webview.umd.js",
				"types": "./dist/index.d.ts"
			},
			"./package.json": "./package.json"
		},
		"main": "dist/@uoslife/webview.es.js",
		"module": "dist/@uoslife/webview.umd.js",
		"types": "dist/index.d.ts",
		"@uoslife:registry": "https://npm.pkg.github.com",
		"directory": "dist"
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview"
	},
	"devDependencies": {
		"@types/react": "^18.2.0",
		"@types/react-dom": "^18.2.17",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"eslint": "^8.55.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-native": "^0.72.2",
		"react-native-web": "^0.19.7",
		"react-native-webview": "^13.6.4",
		"typescript": "^5.2.2",
		"uuid-random": "^1.3.2",
		"vite": "^5.0.8",
		"vite-plugin-dts": "^3.3.1"
	},
	"lerna": {
		"command": {
			"publish": {
				"assets": [
					"package.json",
					{
						"from": "dist/*",
						"to": "dist"
					}
				]
			}
		}
	}
}
